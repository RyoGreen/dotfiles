{
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤",
  "diagnostic.checkCurrentLine": true,

  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.keepCompleteopt": true,
  "suggest.enableSnippets": false,
  "suggest.triggerCharacters": {
    "html": ["<"],
    "default": []
  },
  "suggest.acceptSuggestionOnEnter": "smart",
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.timeout": 5000,
  "suggest.autoTrigger": "always",
  "suggest.enablePreselect": true,

  "prettier.printWidth": 120,
  "prettier.bracketSameLine": true,
  "prettier.html.selfClosingSlash": "never",

  "snippets.ultisnips.pythonPrompt": false,
  "snippets.enable": false,

  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "settings": {
        "gopls": {
          "completionDocumentation": true,
          "usePlaceholders": false,
          "completionBudget": "1s",
          "analyses": {
            "unusedparams": true,
            "shadow": true
          },
          "staticcheck": true,
          "gofumpt": true,
          "hints": {
            "assignVariableTypes": true,
            "compositeLiteralFields": true,
            "compositeLiteralTypes": true,
            "functionTypeParameters": true,
            "parameterNames": true,
            "rangeVariableTypes": true
          }
        }
      }
    },
    "typescript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
      "rootPatterns": ["package.json", "tsconfig.json", "jsconfig.json", ".vim/", ".git/"],
      "settings": {
        "typescript": {
          "inlayHints": {
            "includeInlayParameterNameHints": "all",
            "includeInlayParameterNameHintsWhenArgumentMatchesName": false,
            "includeInlayFunctionParameterTypeHints": true,
            "includeInlayVariableTypeHints": true,
            "includeInlayPropertyDeclarationTypeHints": true,
            "includeInlayFunctionLikeReturnTypeHints": true,
            "includeInlayEnumMemberValueHints": true
          }
        },
        "javascript": {
          "inlayHints": {
            "includeInlayParameterNameHints": "all",
            "includeInlayParameterNameHintsWhenArgumentMatchesName": false,
            "includeInlayFunctionParameterTypeHints": true,
            "includeInlayVariableTypeHints": true,
            "includeInlayPropertyDeclarationTypeHints": true,
            "includeInlayFunctionLikeReturnTypeHints": true,
            "includeInlayEnumMemberValueHints": true
          }
        }
      }
    }
  },

  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "html",
    "markdown",
    "typescriptreact",
    "typescript",
    "javascriptreact",
    "javascript",
    "json",
    "graphql"
  ],

  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.wordBasedSuggestions": false
  },

  "rust-analyzer": {
    "inlayHints": {
      "typeHintsWithVariable": false,
      "chainingHints": false,
      "typeHints": false,
      "parameterHints": false
    },
    "check": {
      "command": "clippy"
    },
    "completion": {
      "callable": {
        "snippets": "none"
      }
    },
    "procMacro": {
      "enable": true
    },
    "rustfmt": {
      "extraArgs": ["+nightly"],
      "rangeFormatting": {
        "enable": true
      }
    },
    "diagnostics": {
      "enable": true
    }
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.snippetSuggestions": "none"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.snippetSuggestions": "none"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.snippetSuggestions": "none"
  },
  "[javascript]": {
    "editor.snippetSuggestions": "none"
  },
  "[javascriptreact]": {
    "editor.snippetSuggestions": "none"
  },
  "[rust]": {
    "editor.snippetSuggestions": "none"
  },

  "tsserver": {
    "enable": true,
    "trace.server": "verbose",
    "preferences": {
      "importModuleSpecifier": "relative",
      "includePackageJsonAutoImports": "off",
      "includeCompletionsForModuleExports": false,
      "includeCompletionsWithSnippetText": false
    }
  }
}

